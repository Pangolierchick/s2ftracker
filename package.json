{
  "name": "twitch",
  "version": "0.0.1",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "source": "./src/main.ts",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true
  },
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/csv-parse": "^1.2.2",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/tmi.js": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "build": "yarn tsup",
    "start": "yarn tsup && node dist/main.js",
    "lint": "eslint .",
    "fix": "eslint --fix --ext .ts .",
    "ts": "tsc --project tsconfig.json",
    "cleanjson": "rm -rf auction.json players.json"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "csv-parse": "^5.2.0",
    "global": "^4.4.0",
    "mongodb": "^4.7.0",
    "node-fetch": "^3.2.6",
    "node-html-parser": "^5.3.3",
    "tslog": "^3.3.3"
  }
}
